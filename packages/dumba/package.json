{
  "name": "dumba",
  "version": "1.0.0",
  "private": false,
  "author": "Ivan Vlatkovic",
  "description": "Mobx forms made easy",
  "buildTarget": "server",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/dumba.esm.js",
  "unpkg": "dist/unpkg/dumba.min.js",
  "jsdelivr": "dist/unpkg/dumba.min.js",
  "sideEffects": false,
  "types": "dist/types",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint --fix \"src/**/\"",
    "lint:check": "eslint \"src/**/\"",
    "format": "prettier --write src/**",
    "format:check": "prettier --check src/**",
    "docs": "node ../../scripts/generate-docs.js",
    "lint:ci": "yarn lint:check && yarn format:check",
    "test": "cross-env BABEL_ENV=test jest --verbose --coverage --no-cache",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:ci": "cross-env BABEL_ENV=test jest --runInBand --ci --coverage --coverageDirectory=coverage",
    "type:check": "del .buildcache/check-types.tsbuildinfo  && tsc -p tsconfig.check-types.json",
    "type:build": "del dist/types && del .buildcache/build-types.tsbuildinfo && tsc -p tsconfig.build-types.json",
    "type:check:watch": "yarn type:check --watch",
    "type:build:watch": "yarn type:build --watch",
    "build": "yarn type:build && node ../../scripts/build-browser --bundle=cjs,esm,umd",
    "build:watch": "node ../../scripts/build-browser --bundle=esm -w",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivandotv/dumba.git"
  },
  "bugs": {
    "url": "https://github.com/ivandotv/dumba/issues"
  },
  "homepage": "https://github.com/ivandotv/dumba#readme",
  "keywords": [],
  "dependencies": {
    "deep-for-each": "^3.0.0",
    "set-value": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "mobx": "^6.3.2",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/types": "^7.14.5",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-define": "^2.0.0",
    "core-js": "^3.15.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4",
    "typedoc": "^0.21.0",
    "typedoc-plugin-markdown": "^3.10.0"
  },
  "peerDependencies": {
    "mobx": "^6.3.2"
  }
}
