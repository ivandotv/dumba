{
  "name": "dumba",
  "version": "2.0.0",
  "private": false,
  "author": "Ivan Vlatkovic",
  "description": "Mobx forms made easy",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/dumba.esm.js",
  "unpkg": "dist/unpkg/dumba.min.js",
  "jsdelivr": "dist/unpkg/dumba.min.js",
  "sideEffects": false,
  "types": "dist/types",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint --fix \"src/**/\"",
    "lint:check": "eslint \"src/**/\"",
    "format": "prettier --write src/**",
    "format:check": "prettier --check src/**",
    "docs": "node ../../scripts/generate-docs.js",
    "lint:ci": "yarn lint:check && yarn format:check",
    "test": "cross-env BABEL_ENV=test jest --verbose --coverage --no-cache",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:ci": "cross-env BABEL_ENV=test jest --runInBand --ci --coverage --coverageDirectory=coverage",
    "type:check": "del .buildcache/check-types.tsbuildinfo  && tsc -p tsconfig.check-types.json",
    "type:build": "del dist/types && del .buildcache/build-types.tsbuildinfo && tsc -p tsconfig.build-types.json",
    "type:check:watch": "yarn type:check --watch",
    "type:build:watch": "yarn type:build --watch",
    "build": "yarn type:build && node ../../scripts/build-browser --bundle=cjs,esm,umd",
    "build:watch": "node ../../scripts/build-browser --bundle=esm -w",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivandotv/dumba.git"
  },
  "bugs": {
    "url": "https://github.com/ivandotv/dumba/issues"
  },
  "homepage": "https://github.com/ivandotv/dumba#readme",
  "keywords": [],
  "dependencies": {
    "deep-for-each": "^3.0.0",
    "set-value": "^4.0.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "mobx": "^6.3.2"
  }
}
