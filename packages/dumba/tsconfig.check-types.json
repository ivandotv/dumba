{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./.buildcache/check-types.tsbuildinfo",
    "noEmit": true,
    "assumeChangesOnlyAffectDirectDependencies": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "**/node_modules",
    "../**/node_modules",
    "../../**/node_modules",
    "src/**/__tests__",
    "src/**/*.test.ts",
    "src/**/*.spec.ts"
  ]
}
